<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE listofactions SYSTEM "actions.dtd">
<listofactions>
   <objecttype name="navigation" class="" resourceBundle="com.ext.resource.NavResource">
        <action name="example"/>
   </objecttype>
   
   <objecttype name="object" class="" resourceBundle="com.ext.resource.NavResource">
       <action name="suggestionExample">
            <command url="com/ext/jsp/example/Suggestion.jsp"/>
        </action>
    </objecttype>
    
    <objecttype name="extNavigator" class="" resourceBundle="com.ext.resource.NavResource">
        <action name="extAction1" >
             <command url="com/ext/jsp/example/HelloWorld.jsp" windowType="page"/>
        </action>
        <action name="extAction2">
            <command url="ptc1/myStuff"/>
            <label>My Stuff</label>
        </action>
    </objecttype>
    
     <objecttype name="object" resourceBundle="com.ext.example.wizard.resource.WizardResource">
          <action name="MR_CREATION_WIZARD" id="MR_CREATION_WIZARD" >
            <command windowType="popup" 
                      url="com/ext/example/wizard/jsp/wizard.jsp" 
                      class="com.ext.example.wizard.form.MRCreationWizardForm" 
                      method="create"/>
          </action>
          <action name="select_element_step" id="select_element_step" >
              <command windowType="popup" url="com/ext/example/wizard/jsp/selectElement.jsp"/>
          </action>  
          <action name="select_operation_step" id="select_operation_step" >
              <command windowType="popup" url="com/ext/example/wizard/jsp/selectOperation.jsp"/>
          </action>  
          <action name="select_template_step" id="select_template_step" >
               <component name="example.selectTemplateBuilder" windowType="page"/> 
          </action>                   
    </objecttype>
    
    
</listofactions>